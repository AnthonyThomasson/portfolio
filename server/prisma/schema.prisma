// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model system_nodes {
  id      Int     @id @default(autoincrement())
  icon    String
  name    String
  type    String
  parent  Int
  content String? @db.Text
}

model technologies {
  id         Int    @id @default(autoincrement())
  name       String
  icon       String
  experience String
}
